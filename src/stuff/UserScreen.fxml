      <ListView fx:id="albumsListView" layoutX="65.0" layoutY="89.0" prefHeight="418.0" prefWidth="200.0" />
      <Label fx:id="albumsListViewLabel" layoutX="65.0" layoutY="72.0" text="Albums:" />
