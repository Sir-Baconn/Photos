      <Label fx:id="earliestDateLabel" layoutX="322.0" layoutY="155.0" text="Date of Earliest Photo :" />
      <Label fx:id="latestDateLabel" layoutX="322.0" layoutY="180.0" text="Date of Latest Photo :" />
