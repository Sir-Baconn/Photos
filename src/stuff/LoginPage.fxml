<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<Pane fx:id="layout" prefHeight="450.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stuff.LoginController">
   <children>
      <Label fx:id="logLabel" layoutX="300.0" layoutY="100.0" text="Login" />
      <Label fx:id="userLabel" layoutX="100.0" layoutY="200.0" text="Username" />
      <Label fx:id="passLabel" layoutX="100.0" layoutY="300.0" text="Password" />
      <Button fx:id="loginBut" layoutX="200.0" layoutY="400.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Button fx:id="createBut" layoutX="300.0" layoutY="400.0" mnemonicParsing="false" onAction="#createAccount" text="Create Account" />
      <TextField fx:id="userTextField" layoutX="200.0" layoutY="200.0" promptText="Username">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordTextField" layoutX="200.0" layoutY="300.0" promptText="Password">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </PasswordField>
   </children>
</Pane>
